{
  "name": "jotai-state-tree",
  "version": "1.1.1",
  "description": "MobX-State-Tree API compatible library powered by Jotai",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/react.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [
    "jotai",
    "mobx-state-tree",
    "mst",
    "state-management",
    "react",
    "typescript",
    "state-tree"
  ],
  "author": "Brandon Martel",
  "license": "MIT",
  "peerDependencies": {
    "jotai": ">=2.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.1",
    "@types/react": "^18.2.0",
    "eslint": "^9.39.2",
    "jotai": "^2.6.0",
    "jsdom": "^27.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "semantic-release": "^25.0.2",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmartel/jotai-state-tree"
  }
}
